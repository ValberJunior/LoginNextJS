{
  "name": "LoginNextJS",
  "version": "0.1.0",
  "description": "An example screen created with NextJS and Sass",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "web-scripts test --config jest.config.js",
    "pretest:ci": "yarn lint",
    "test:ci": "yarn test --ci --coverage --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@next/font": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@zeit/next-typescript": "latest",
    "babel-plugin-styled-components": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "sass": "latest",
    "styled-components": "latest",
    "styled-reset": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@spotify/tsconfig": "latest",
    "@spotify/web-scripts": "latest",
    "@testing-library/react": "latest",
    "@types/styled-components": "latest",
    "husky": "latest",
    "jest-junit": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "web-scripts precommit"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
